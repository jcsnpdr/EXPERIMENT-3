#include<iostream>

using namespace std;

int main()
{
int s, i, q, w;
char arr[s];

cout << "Enter the desired number of elements: "; cin >> s;

cout << "enter the elements you want to be in reserved: ";

for (i = 0;i < s; i++)
{
cin >> arr[i];
}

	q = i-1;
	i = 0;
	
while (i < q)
{
w = arr[i];
arr[i] = arr[q];
arr[q] = w;
i++;
q--;
}

cout << endl;
for (i = 0;i < s ;i++)
{
cout << arr[i] << " ";
}


return 0;
}
