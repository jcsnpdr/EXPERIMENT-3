#include<iostream>

using namespace std;

int main()
{
int s, w;
int a, b;
char arr[s];


cout << "enter the desired number of elements: ";
cin >> s;
cout << endl;

cout << "enter the elements you want to be in reserved: ";
for (a = 0 ; a < s; a++)
{
	cin >> arr[a];
}
 	b = s-1;
 	a =0;
 	
while (a < b)
{
	w = arr[a];
	arr[a] = arr[b];
	arr[b] = w;
	a++;
	b--;
}

cout << endl;
for ( a=0; a < s; a++)
{
	cout << arr[a] << " ";

}

return 0;
}

